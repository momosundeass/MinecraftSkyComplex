<!-- Alloying -->
  <recipe name="Triple: Iron" required="true">
    <alloying energy="20000" exp="0.5">
      <input name="POWDER_IRON"/>
      <input name="contenttweaker:me_iron"/>
      <output name="minecraft:iron_ingot" amount="3"/>
